{
  "name": "upfront-editable",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/upfrontIO/editable.js.git"
  },
  "keywords": [
    "contenteditable",
    "editable"
  ],
  "authors": [
    "Matteo Agosti <matteo@upfront.io>",
    "Lukas Peyer <lukas@upfront.io>"
  ],
  "main": "src/core.js",
  "description": "Friendly contenteditable API",
  "license": "MIT",
  "dependencies": {
    "bowser": "0.7.3",
    "jquery": "^2.1.4",
    "rangy": "^1.3.0"
  },
  "scripts": {
    "build": "browserify src/core.js ---standalone Editable -d > editable.js",
    "release": "browserify src/core.js ---standalone Editable | uglifyjs -m -c > editable.min.js",
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "rangy": "global:rangy",
    "jquery": "global:jQuery"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.7.0",
    "grunt-bump": "^0.1.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-coffee": "^0.12.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-git-revision": "0.0.1",
    "grunt-karma": "^0.10.1",
    "grunt-react": "^0.12.2",
    "grunt-replace": "^0.8.0",
    "grunt-shell": "^1.1.1",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "load-grunt-tasks": "^1.0.0",
    "matchdep": "~0.1.1",
    "uglify-js": "^2.4.24"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE",
    "editable.js",
    "editable.min.js"
  ]
}
