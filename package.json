{
  "name": "upfront-editable",
  "description": "Friendly contenteditable API",
  "version": "0.0.0-development",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bowser": "1.4.6",
    "rangy": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.1",
    "gh-pages": "^0.11.0",
    "jasmine-core": "^2.2.0",
    "jquery": "^2.2.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "normalize.css": "^4.1.1",
    "open-browser-webpack-plugin": "0.0.2",
    "prismjs": "^1.4.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "src",
    "lib",
    "dist"
  ],
  "keywords": [
    "contenteditable",
    "editable"
  ],
  "license": "MIT",
  "main": "lib/core.js",
  "peerDependencies": {
    "jquery": "^2.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/livingdocsIO/editable.js.git"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "prebuild:dist": "rimraf ./coverage",
    "build:dist": "BUILD_DIST=true webpack",
    "prebuild:docs": "rimraf ./examples/dist",
    "build:docs": "BUILD_DOCS=true webpack",
    "build:lib": "babel src --out-dir lib",
    "prebuild:lib": "rimraf ./{lib,coverage}",
    "build": "npm run build:dist -s && npm run build:lib -s && npm run build:docs -s",
    "test": "npm run build -s && npm run test:karma -s",
    "posttest": "npm run lint -s",
    "test:all": "npm run test:karma -s -- --browsers Chrome,Firefox,Safari",
    "test:karma": "karma start",
    "test:watch": "npm run test:karma -s -- --single-run false",
    "lint": "eslint --ignore-path .gitignore .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postsemantic-release": "node ./bin/deploy-example.js",
    "coverage": "cat coverage/lcov/lcov.info | coveralls"
  }
}
